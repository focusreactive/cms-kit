{
  "name": "storyblok",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port=4040",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "proxy": "local-ssl-proxy --source 4050 --target 4040 --cert localhost.pem --key localhost-key.pem",
    "pull-schemas": "storyblok pull-components --space 293915 --file-name production --path src/contentSections/presets/",
    "push-schemas": "storyblok push-components --space ###### src/contentSections/presets/components.production.json --presets-source src/contentSections/presets/presets.production.json"
  },
  "dependencies": {
    "@shared/ui": "workspace:*",
    "@storyblok/react": "^3.0.10",
    "next": ">=14.1.1",
    "react": "^18",
    "react-dom": "^18",
    "storyblok-rich-text-react-renderer": "^2.9.1"
  },
  "devDependencies": {
    "@shared/eslint-config": "workspace:*",
    "@shared/tailwind-config": "workspace:*",
    "@shared/ts-config": "workspace:*",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "local-ssl-proxy": "^2.0.5",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwindcss": "^3.4.3",
    "typescript": "5.4.5"
  }
